{
  "templates": {
    "TCSTK-case-manager-app": {
      "displayName": "Case Manager App",
      "enabled": true,
      "templateFolder": "/applicationTemplate/TCSTK-case-manager-app",
      "useGit": false,
      "replacements": [
        {
          "from": "tcstk-case-manager-app",
          "to": "@name"
        },
        {
          "from": "Case Manager",
          "to": "@name"
        }
      ]
    },"TCSTK-case-manager-app-git": {
      "displayName": "Case Manager App (GIT)",
      "enabled": true,
      "templateFolder": "/applicationTemplate/TCSTK-case-manager-app",
      "useGit": true,
      "git" : "https://github.com/TIBCOSoftware/TCSTK-case-manager-app",
      "gitTag": "LATEST",
      "removeGitFolder": true,
      "gitPostCommands": ["rm package-lock.json"],
      "replacements": [
        {
          "from": "tcstk-case-manager-app",
          "to": "@name"
        },
        {
          "from": "Case Manager",
          "to": "@name"
        }
      ]
    },
    "Form-Template-Git": {
      "displayName": "First Template",
      "enabled": false,
      "templateFolder": "",
      "useGit": true,
      "git" : "https://github.com/TIBCOSoftware/TCSTK-custom-form-app-plugin",
      "gitTag": "LATEST",
      "removeGitFolder": true,
      "gitPostCommands": ["rm package-lock.json"],
      "replacements": [
        {
          "from": "custom-form-app",
          "to": "@name"
        }
      ]
    },
    "Template2": {
      "displayName": "Second Template",
      "enabled": false,
      "templateFolder": "/applicationTemplate/template2",
      "useGit": false,
      "git" : "",
      "replacements": [
        {
          "from": "String1",
          "to": "ToString1"
        },
        {
          "from": "String2",
          "to": "ToString2"
        }
      ]
    }
  },
  "useDebug": true
}

