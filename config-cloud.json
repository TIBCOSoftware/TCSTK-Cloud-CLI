{
  "endpoints": {
    "_note": "ALL ENDPOINTS START AFTER THE REGION",
    "account_info": "account.cloud.tibco.com/tsc-ws/v1/my-accounts/info",
    "general_login": "account.cloud.tibco.com/idm/v3/login-oauth",
    "generate_oauth": "account.cloud.tibco.com/idm/v1/oauth2/tokens/operations/generate",
    "get_oauth": "account.cloud.tibco.com/idm/v1/oauth2/tokens",
    "revoke_oauth": "account.cloud.tibco.com/idm/v1/oauth2/tokens/operations/revoke",
    "org_folders": "liveapps.cloud.tibco.com/webresource/v1/orgFolders/"
  },
  "mappings": {
    "oauth": {
      "entries": [
        {
          "header": "Name",
          "field": "name"
        },
        {
          "header": "Tenant",
          "field": "tenantId"
        },
        {
          "header": "Created On",
          "field": "generatedTime",
          "format": "DATE"
        },
        {
          "header": "Expires On",
          "field": "expirationTime",
          "format": "DATE"
        },
        {
          "header": "Used",
          "field": "used"
        },
        {
          "header": "Last Accessed On",
          "field": "lastAccessed",
          "format": "DATE"
        }
      ]
    },
    "org_folder": {
      "entries": [
        {
          "header": "Name",
          "field": "name"
        },
        {
          "header": "Created",
          "field": "creationDate",
          "format": "DATE"
        },
        {
          "header": "Modified",
          "field": "lastModifiedDate",
          "format": "DATE"
        }
      ]
    },
    "tci_apps": {
      "entries": [
        {
          "header": "Name",
          "field": "name"
        },
        {
          "header": "Type",
          "field": "type.val"
        },
        {
          "header": "Visibility",
          "field": "endpointVisibility"
        },
        {
          "header": "publishStage",
          "field": "publishStage"
        },
        {
          "header": "Last Modified",
          "field": "modifiedTime",
          "format": "DATE"
        },
        {
          "header": "Status",
          "field": "appInitialStatus"
        },
        {
          "header": "Instances",
          "field": "desiredInstanceCount"
        }
      ]
    }
  }
}

